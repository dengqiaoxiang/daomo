<view class="content"><view class="row b-b"><text class="tit">联系人</text><input class="input" type="text" placeholder="收货人姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','contactor_name','$event',[]],['form']]]]]}}" value="{{form.contactor_name}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">手机号</text><input class="input" type="number" placeholder="收货人手机号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['form']]]]]}}" value="{{form.phone}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">地址</text><view class="address-box"><area-select bind:submit="__e" bind:input="__e" vue-id="0e0a4db1-1" value="{{form.address_id}}" data-event-opts="{{[['^submit',[['changeAddress']]],['^input',[['__set_model',['$0','address_id','$event',[]],['form']]]]]}}" bind:__l="__l"></area-select></view></view><view class="row b-b"><text class="tit">街道</text><input class="input" type="text" data-event-opts="{{[['input',[['__set_model',['$0','street','$event',[]],['form']]]]]}}" value="{{form.street}}" bindinput="__e"/></view><view class="row default-row"><text class="tit">设为默认</text><switch checked="{{form.is_default==1}}" color="#fa436a" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">提交</button></view>